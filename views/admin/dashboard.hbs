              <script>
                
                
                    function deleteTourist(id){
                     
                      swal({
                           title: "Are you sure?",
                           text: "Once deleted, you will not be able to recover !",
                           icon: "warning",
                           buttons: true,
                           dangerMode: true,
                         })
                         .then((willDelete) => {
                           if (willDelete) {
                             fetch(`/admin/dashboard/delete-user/${id}`);
                             swal("Deleted Successfully",{
                               icon: "success",
                             });

                           } else {
                             swal("Account is safe!");
                           }
                       });

                    }
